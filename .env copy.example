# OpenRouter API Configuration
VITE_OPENROUTER_API_KEY=

# Optional: Specify which model to use (defaults to DeepSeek v3)
VITE_OPENROUTER_MODEL=deepseek/deepseek-chat-v3-0324:free

# Optional: Your app name for OpenRouter analytics
VITE_OPENROUTER_APP_NAME=Anger Translator

# ElevenLabs Text-to-Speech Configuration
VITE_ELEVENLABS_API_KEY=

# Optional: Default voice settings
VITE_ELEVENLABS_DEFAULT_VOICE=pNInz6obpgDQGcFmaJgB

# Security Configuration
VITE_FORCE_HTTPS=true
VITE_DISABLE_RIGHT_CLICK=false
VITE_SECURITY_ENDPOINT=

# Development Settings
VITE_ENABLE_DEBUG_LOGS=false
VITE_MOCK_API_RESPONSES=false